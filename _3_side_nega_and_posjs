// Input: -12, 11, -13, -5, 6, -7, 5, -3, -6
// Output: -12 -13 -5 -7 -3 -6 11 6 5

let arr=[ -12, 11, -13, -5, 6, -7, 5, -3, -6]
let arr2=[]
let ind=0

for(let i=0;i<arr.length;i++)
{
    if(arr[i]<0)
    {
        arr2[ind]=arr[i]
        ind++

    }
}
for(let j=0;j<arr.length;j++)
{
    if(arr[j]>=0)
    {
        arr2[ind]=arr[j]
        ind++
    }
}

console.log(arr2)